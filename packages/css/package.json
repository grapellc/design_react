{
  "name": "@grape_design_react/css",
  "version": "1.2.3",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/daangn/grape_design_react.git",
    "directory": "packages/css"
  },
  "scripts": {
    "generate": "bun rootage:generate && qvism:generate",
    "rootage:generate": "bun rootage token-ts ./vars --prefix seed && rootage component-spec ./vars/component --prefix seed",
    "qvism:generate": "bun qvism ./ ./recipes",
    "lint:publish": "bun publint"
  },
  "sideEffects": [
    "*.css"
  ],
  "type": "module",
  "exports": {
    "./*.css": "./*.css",
    "./base.css": "./base.css",
    "./base.min.css": "./base.min.css",
    "./all.css": "./all.css",
    "./all.min.css": "./all.min.css",
    "./recipes/*.css": "./recipes/*.css",
    "./recipes/*": {
      "types": "./recipes/*.d.ts",
      "import": "./recipes/*.mjs"
    },
    "./vars": {
      "types": "./vars/index.d.ts",
      "import": "./vars/index.mjs"
    },
    "./vars/*": {
      "types": "./vars/*.d.ts",
      "import": "./vars/*.mjs"
    },
    "./vars/component": {
      "types": "./vars/component/index.d.ts",
      "import": "./vars/component/index.mjs"
    },
    "./vars/component/*": {
      "types": "./vars/component/*.d.ts",
      "import": "./vars/component/*.mjs"
    },
    "./theming": {
      "types": "./theming/index.d.ts",
      "import": "./theming/index.mjs",
      "require": "./theming/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "package.json",
    "recipes",
    "vars",
    "theming",
    "utils",
    "*.css"
  ],
  "devDependencies": {
    "@grape_design_react/qvism-cli": "0.0.0",
    "@grape_design_react/qvism-preset": "0.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
