{
  "name": "@grape_design_react/figma",
  "version": "1.3.4",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/daangn/grape_design_react.git",
    "directory": "packages/figma"
  },
  "author": "Asher <asher@daangn.com>",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "import": "./lib/index.js",
      "require": "./lib/index.cjs"
    },
    "./codegen": {
      "types": "./lib/codegen/index.d.ts",
      "import": "./lib/codegen/index.js",
      "require": "./lib/codegen/index.cjs"
    },
    "./codegen/targets/react": {
      "types": "./lib/codegen/targets/react/index.d.ts",
      "import": "./lib/codegen/targets/react/index.js",
      "require": "./lib/codegen/targets/react/index.cjs"
    }
  },
  "main": "./lib/index.cjs",
  "sideEffects": false,
  "files": [
    "lib",
    "src"
  ],
  "scripts": {
    "clean": "rm -rf lib",
    "build": "bunchee",
    "lint:publish": "bun publint",
    "sync-entities": "find src/entities/data/__generated__ -mindepth 1 -maxdepth 1 ! -name 'archive' -exec rm -rf {} + 2>/dev/null; bun figma-extractor src/entities/data/__generated__"
  },
  "dependencies": {
    "@grape_design_react/css": "workspace:*",
    "change-case": "^5.4.4",
    "ts-pattern": "^5.7.0"
  },
  "devDependencies": {
    "@figma/plugin-typings": "^1.110.0",
    "@figma/rest-api-spec": "^0.36.0",
    "@karrotmarket/icon-data": "^1.17.0",
    "@grape_design_react/figma-extractor": "workspace:*",
    "typescript": "^5.9.2"
  },
  "publishConfig": {
    "access": "public"
  }
}
