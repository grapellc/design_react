{
  "name": "@grape_design_react/docs-mcp",
  "version": "0.5.1",
  "description": "The official MCP server for SEED Design documentation",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/daangn/grape_design_react.git",
    "directory": "packages/docs-mcp"
  },
  "homepage": "https://grape_design_react.io/",
  "license": "MIT",
  "type": "module",
  "sideEffects": false,
  "files": [
    "dist",
    "src"
  ],
  "bin": {
    "seed-docs-mcp": "./dist/stdio.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./server": {
      "types": "./dist/server.d.ts",
      "import": "./dist/server.js"
    },
    "./tools": {
      "types": "./dist/tools/index.d.ts",
      "import": "./dist/tools/index.js"
    },
    "./stdio": {
      "import": "./dist/stdio.js"
    }
  },
  "scripts": {
    "clean": "rm -rf dist",
    "build": "bunchee",
    "dev": "bun ./dist/stdio.js",
    "lint": "biome check ./src",
    "lint:fix": "biome check --fix ./src",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "bun run build"
  },
  "dependencies": {
    "@karrotmarket/icon-data": "^1.17.0",
    "@modelcontextprotocol/sdk": "^1.15.0",
    "lru-cache": "^11.0.2",
    "zod": "^3.24.3"
  },
  "devDependencies": {
    "@types/node": "^25.0.0",
    "bunchee": "^6.3.2",
    "typescript": "^5.8.3"
  },
  "keywords": [
    "grape_design_react",
    "mcp-server",
    "mcp",
    "ai",
    "documentation"
  ],
  "publishConfig": {
    "access": "public"
  }
}
