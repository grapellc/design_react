{
  "name": "@grape_design_react/webpack-plugin",
  "version": "1.1.1",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/daangn/grape_design_react.git",
    "directory": "packages/webpack-plugin"
  },
  "author": "Asher <asher@daangn.com>",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "types": "./lib/index.d.ts",
        "default": "./lib/index.js"
      },
      "require": {
        "types": "./lib/index.d.cts",
        "default": "./lib/index.cjs"
      }
    }
  },
  "main": "./lib/index.cjs",
  "types": "./lib/index.d.ts",
  "sideEffects": false,
  "files": [
    "lib",
    "src"
  ],
  "scripts": {
    "clean": "rm -rf lib",
    "build": "bunchee",
    "lint:publish": "bun publint"
  },
  "dependencies": {},
  "devDependencies": {
    "@rspack/core": "^1.3.6",
    "@grape_design_react/css": "workspace:*",
    "html-webpack-plugin": "^5.6.3",
    "webpack": "^5.99.6"
  },
  "peerDependencies": {
    "@rspack/core": "^1.2.0",
    "@grape_design_react/css": "workspace:*",
    "html-webpack-plugin": "^5.0.0",
    "webpack": "^5.0.0"
  },
  "peerDependenciesMeta": {
    "webpack": {
      "optional": true
    },
    "html-webpack-plugin": {
      "optional": true
    },
    "@rspack/core": {
      "optional": true
    }
  },
  "publishConfig": {
    "access": "public"
  }
}
