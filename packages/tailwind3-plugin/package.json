{
  "name": "@grape_design_react/tailwind3-plugin",
  "version": "1.1.17",
  "description": "Tailwind3 CSS plugin for Seed Design tokens",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/daangn/grape_design_react.git",
    "directory": "packages/tailwind3-plugin"
  },
  "author": "Daangn",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "types": "./lib/index.d.ts",
        "default": "./lib/index.js"
      },
      "require": {
        "types": "./lib/index.d.cts",
        "default": "./lib/index.cjs"
      }
    }
  },
  "main": "./lib/index.cjs",
  "module": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "sideEffects": false,
  "files": [
    "lib",
    "src"
  ],
  "scripts": {
    "clean": "rm -rf lib",
    "build": "bunchee",
    "rootage:generate": "rootage tailwind3-plugin ./src --prefix seed",
    "lint:publish": "bun publint"
  },
  "keywords": [
    "tailwindcss",
    "grape_design_react",
    "design-tokens",
    "plugin"
  ],
  "devDependencies": {
    "tailwindcss": "3"
  },
  "peerDependencies": {
    "tailwindcss": "3"
  },
  "publishConfig": {
    "access": "public"
  }
}
